\begin_layout Standard
\begin_inset Formula 
\[
\boldsymbol{q}=\left[\begin{array}{c}
q_{1}\\
q_{2}\\
\vdots\\
q_{n}
\end{array}\right]
\]

\end_inset

 Avec cette notation, il vient
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathcal{P}(\boldsymbol{q})=\sum_{i=1}^{n}\sum_{j=1}^{n}\int_{\Omega}\frac{1}{2}\cdot\boldsymbol{\epsilon}(\boldsymbol{\Phi}_{i})\cdot q_{i}\cdot\boldsymbol{H}\cdot\boldsymbol{\epsilon}(\boldsymbol{\Phi}_{j})\cdot q_{j}\cdot d\Omega
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathcal{P}(\boldsymbol{q})=\frac{1}{2}\cdot\boldsymbol{q}^{t}\cdot\boldsymbol{K}\cdot\boldsymbol{q}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
K_{ij}=\int_{\Omega}\boldsymbol{\epsilon}(\boldsymbol{\Phi}_{i})\cdot\boldsymbol{H}\cdot\boldsymbol{\epsilon}(\boldsymbol{\Phi}_{j})\cdot d\Omega
\]

\end_inset


\end_layout

\begin_layout Standard
Travail des forces extérieures :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\boldsymbol{f}\bar{\otimes}\boldsymbol{u}=\boldsymbol{F}^{t}\cdot\boldsymbol{q}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
F_{i}=\int_{\Omega}\boldsymbol{\Phi}_{i}
